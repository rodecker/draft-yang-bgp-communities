{
  "ietf-bgp-communities:bgp-communities": {
    "description": "BGP Communities for Example.net",
    "contact": [
      {
        "email-address": "noc@example.net",
        "name": "Example.net contact",
        "role": "Administrative contact",
        "organization": "Example.net",
        "organizational-unit": "NOC"
      }
    ],
    "uri": "http://example.net/peering/",
    "serial": 2023080101,
    "regular": [
      {
        "name": "NO-ANNOUNCE",
        "description": "Example Regular: Do not announce to peers",
        "global-admin": 65001,
        "local-admin": {
          "field": [
            {
              "name": "Action",
              "pattern": "3000",
              "description": "No announce to peers"
            }
          ]
        }
      },
      {
        "name": "PREPEND-TO-NA",
        "description": "Example Regular: Prepend X times to North American peers",
        "global-admin": 65001,
        "local-admin": {
          "field": [
            {
              "name": "Continent",
              "length": 3,
              "pattern": "200",
              "description": "North America"
            },
            {
              "name": "Prepend-count",
              "length": 1,
              "pattern": "[1-5]",
              "description": "*"
            }
          ]
        }
      },
      {
        "name": "PREPEND-TO-EU",
        "description": "Example Regular: Prepend X times to European peers",
        "global-admin": 65001,
        "local-admin": {
          "field": [
            {
              "name": "Continent",
              "length": 3,
              "pattern": "210",
              "description": "Europe"
            },
            {
              "name": "Prepend-count",
              "length": 1,
              "pattern": "[1-5]",
              "description": "*"
            }
          ]
        }
      },
      {
        "name": "RFC4384-EU-NL",
        "description": "Example Regular: RFC4384 - Origin EU/NL",
        "global-admin": 65001,
        "local-admin": {
          "format": "binary",
          "field": [
            {
              "name": "Region",
              "length": 5,
              "pattern": "00101",
              "description": "EU"
            },
            {
              "name": "Satellite",
              "length": 1,
              "pattern": "[0-1]",
              "description": "*"
            },
            {
              "name": "Country",
              "length": 10,
              "pattern": "1000010000",
              "description": "NL"
            }
          ]
        }
      }
    ],
    "large": [
      {
        "name": "RFC8195-ORIGIN-NL",
        "category": "informational",
        "description": "Example Large: RFC8195 - Origin NL",
        "global-admin": 65001,
        "local-data-part-1": {
          "field": [
            {
              "name": "Function",
              "pattern": "1",
              "description": "Origin"
            }
          ]
        },
        "local-data-part-2": {
          "field": [
            {
              "name": "Country",
              "pattern": "528",
              "description": "NL"
            }
          ]
        }
      },
      {
        "name": "RFC8195-NOEXPORT-ASN",
        "category": "action",
        "description": "Example Large: RFC8195 - No-export to ASN",
        "global-admin": 65001,
        "local-data-part-1": {
          "field": [
            {
              "name": "Function",
              "pattern": "4",
              "description": "ASN-No-Export"
            }
          ]
        },
        "local-data-part-2": {
          "field": [
            {
              "name": "ASN",
              "pattern": ".*",
              "description": "*"
            }
          ]
        }
      }
    ],
    "extended": [
      {
        "name": "ANNOUNCE-TO-PEER",
        "description": "Example Extended: Announce to Peer AS",
        "type": 0,
        "subtype": 3,
        "asn": 65001,
        "local-admin": {
          "field": [
            {
              "name": "Peer-AS",
              "pattern": ".*",
              "description": "*"
            }
          ]
        }
      }
    ]
  }
}
